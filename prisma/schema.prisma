// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id
  summonerName String
  totalWin Int
  totalLose Int
  rank	String
  tier String
  champ Champ[]
}

model Champ{
  id Int //championId
  user User @relation(fields: [userId], references:[id])
  userId Int @unique
  win Int
  lose Int
  kill Int
  death Int
  assist Int
}